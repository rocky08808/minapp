<view class="page">
  <view class="canvas-view">
    <canvas 
      id="canvas"
      width="610"
      height="610"
      class="canvas"
      onTouchStart="log"
      onTouchMove="log"
      onTouchEnd="log"
      style="height: 200px;"
      disable-scroll="true"
      onTap="draw"
      onTouchCancel="log"
      onLongTap="draw"
    />
  </view>
</view>


